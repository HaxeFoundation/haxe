{
	"files.associations": {
		"*.mly": "ocaml",
		"*.ml": "ocaml",
		"Makefile.*": "makefile"
	},
	"[ocaml]": {
		"editor.tabSize": 4
	},
	"files.exclude": {
		"**/_build": true
	},
	"reason.server.languages": ["ocaml"],
	"haxe.diagnosticsPathFilter": "${workspaceRoot}/std",
	"npm.autoDetect": "off"
}